<constraint name="fk_provider_id" type="fk-constr"	 comparison-type="MATCH FULL"
	 upd-action="NO ACTION" del-action="NO ACTION" ref-table="public.fornecedor" table="jogosps3.&quot;PS3_Game&quot;">
	<columns names="id_provider" ref-type="src-columns"/>
	<columns names="idFornecedor" ref-type="dst-columns"/>
</constraint>
<constraint name="fk_shop_id" type="fk-constr"	 comparison-type="MATCH FULL"
	 upd-action="NO ACTION" del-action="NO ACTION" ref-table="public.loja" table="jogosps3.&quot;PS3_Game&quot;">
	<columns names="id_shop" ref-type="src-columns"/>
	<columns names="idLoja" ref-type="dst-columns"/>
</constraint>
<constraint name="fk_manufacturer_id" type="fk-constr"	 comparison-type="MATCH FULL"
	 upd-action="NO ACTION" del-action="NO ACTION" ref-table="public.fabricante" table="jogosps3.&quot;PS3_Game&quot;">
	<columns names="id_manufacturer" ref-type="src-columns"/>
	<columns names="idFabricante" ref-type="dst-columns"/>
</constraint>
<constraint name="fk_user_CPF" type="fk-constr"	 comparison-type="MATCH FULL"
	 upd-action="NO ACTION" del-action="NO ACTION" ref-table="public.usuario" table="jogosps3.profile">
	<columns names="CPF" ref-type="src-columns"/>
	<columns names="CPF" ref-type="dst-columns"/>
</constraint>
<constraint name="fk_friendship_id" type="fk-constr"	 comparison-type="MATCH FULL"
	 upd-action="NO ACTION" del-action="NO ACTION" ref-table="public.usuario" table="public.friendship">
	<columns names="usuario_idUsuario,usuario_idAmigo" ref-type="src-columns"/>
</constraint>
<relationship name="&quot;rel_PS3_Game_provider&quot;" type="relfk"
	 src-table="jogosps3.&quot;PS3_Game&quot;"
	 dst-table="public.fornecedor"
	 src-required="true" dst-required="true"/>

<relationship name="&quot;rel_PS3_Game_shop&quot;" type="relfk"
	 src-table="jogosps3.&quot;PS3_Game&quot;"
	 dst-table="public.loja"
	 src-required="true" dst-required="true"/>

<relationship name="&quot;rel_PS3_Game_manufacturer&quot;" type="relfk"
	 src-table="jogosps3.&quot;PS3_Game&quot;"
	 dst-table="public.fabricante"
	 src-required="true" dst-required="true"/>

<relationship name="rel_profile_user" type="relfk"
	 src-table="jogosps3.profile"
	 dst-table="public.usuario"
	 src-required="true" dst-required="true"/>

<relationship name="rel_friendship_user" type="relfk"
	 src-table="public.friendship"
	 dst-table="public.usuario"
	 src-required="true" dst-required="true"/>

</dbmodel>
