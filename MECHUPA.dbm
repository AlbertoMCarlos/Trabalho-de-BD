<dbmodel>
<schema name="public" protected="true" fill-color="#e1e1e1" sql-disabled="true">
</schema>

<schema name="jogosps3" rect-visible="true" fill-color="#e1e1e1">
</schema>

<database name="tp01_2013_01">
</database>

<table name="PS3_Game">
	<schema name="jogosps3"/>
	<comment><![CDATA[Tabela do Produto: PS3]]></comment>
	<position x="55" y="288"/>
	<column name="id" not-null="true">
		<type name="smallint"/>
	</column>
	<column name="name">
		<type name="varchar" length="50"/>
	</column>
	<column name="newest">
		<type name="smallint" length="50"/>
	</column>
	<column name="gen">
		<type name="smallint" length="50"/>
	</column>
	<column name="price">
		<type name="real"/>
	</column>
	<column name="map_price">
		<type name="smallint" length="10"/>
	</column>
	<column name="gameSex">
		<type name="smallint"/>
	</column>
	<column name="id_provider">
		<type name="smallint"/>
	</column>
	<column name="id_manufacturer">
		<type name="smallint"/>
	</column>
	<column name="id_shop">
		<type name="smallint"/>
	</column>
	<constraint name="pk_product_id" type="pk-constr" factor="10" table="jogosps3.&quot;PS3_Game&quot;">
		<columns names="id" ref-type="src-columns"/>
	</constraint>
	<constraint name="ck_product_create" type="ck-constr" table="jogosps3.&quot;PS3_Game&quot;">
			<expression><![CDATA[gen EXISTS IN (SELECT code FROM feature WHERE type='gen') and map_price EXISTS IN (SELECT code FROM feature WHERE type='map_price') and newest EXISTS IN (SELECT code FROM feature WHERE type='newest')]]></expression>
	</constraint>
</table>

